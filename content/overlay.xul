  <popupset id="editorTooltipSet">
     <panel id="gotoTab_panel"
              onpopupshown="document.getElementById('gotoTab_textbox').focus(); document.getElementById('gotoTab_error_hbox').setAttribute('collapsed', 'true'); ">
           <vbox id="gotoTab_vbox" maxwidth="420">
               <description id="gotoTab_description">&gotoTab.prompt.description;</description>
               <hbox id="gotoTab_hbox" align="center">
                   <label id="gotoTab_label" value="&gotoTab.prompt.label;" />
                   <textbox id="gotoTab_textbox"
                            onkeypress="ko.extensions.qwin.onHotkeyGotoTab_onkeypress_handler(event);" />
               </hbox>
               <hbox id="gotoTab_error_hbox"
                     align="center"
                     collapsed="true">
                   <image id="gotoTab_error_image" />
                   <label id="gotoTab_error_label" />
               </hbox>
           </vbox>
     </panel>
  </popupset>

  <popupset id="extensionPopupSet">
    <menupopup id="qwin-tree-contextmenu">
      <menuitem id="qwin-tree-contextmenu-showViewCmd"
                label="&showViewCmd.label;"
                accesskey="&showViewCmd.key;"
                tooltiptext="&showViewCmd.tip;"
                oncommand="ko.extensions.qwin.doCommand('showView');"/>
      <menuitem id="qwin-tree-contextmenu-closeViewCmd"
                label="&closeViewCmd.label;"
                accesskey="&closeViewCmd.key;"
                tooltiptext="&closeViewCmd.tip;"
                oncommand="ko.extensions.qwin.doCommand('closeView');"/>
      <menuseparator/>
      <menuitem id="qwin-tree-contextmenu-moveToOtherTabGroupCmd"
                label="&mvToOtherTabGrpCmd.label;"
                accesskey="&mvToOtherTabGrpCmd.key;"
                tooltiptext="&mvToOtherTabGrpCmd.tip;"
                observes="cmd_movetab"/>
      <menuseparator/>
      <menuitem id="qwin-tree-contextmenu-closeAllOthersCmd"
                label="&closeAllOthersCmd.label;"
                accesskey="&closeAllOthersCmd.key;"
                tooltiptext="&closeAllOthersCmd.tip;"
                oncommand="ko.extensions.qwin.doCommand('closeAllOthers');"/>
      <menuitem id="qwin-tree-contextmenu-closeAllCmd"
                label="&closeAllCmd.label;"
                accesskey="&closeAllCmd.key;"
                tooltiptext="&closeAllCmd.tip;"
                observes="cmd_closeAll"/>
      <menuseparator/>
      <menuitem id="qwin-tree-contextmenu-showPropsDlgCmd"
